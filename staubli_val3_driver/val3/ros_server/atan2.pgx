<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="atan2" access="public">
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1">
      <Parameter name="x_nY" type="num" xsi:type="element" />
      <Parameter name="x_nX" type="num" xsi:type="element" />
      <Parameter name="x_nResult" type="num" xsi:type="element" use="reference" />
    </Parameters>
    <Code><![CDATA[begin
  if x_nX > 0
      x_nResult = atan(x_nY / x_nX)
  elseIf (x_nX < 0) and (x_nY >= 0)
      x_nResult = atan(x_nY / x_nX) + 180.0
  elseIf (x_nX < 0) and (x_nY < 0)
      x_nResult = atan(x_nY / x_nX) - 180.0
  elseIf (x_nX==0) and (x_nY > 0)
      x_nResult = 90.0
  elseIf (x_nX==0) and (x_nY < 0)
      x_nResult = -90.0
  elseIf (x_nX==0) and (x_nY==0)
      x_nResult = 0
  endIf
end]]></Code>
  </Program>
</Programs>
