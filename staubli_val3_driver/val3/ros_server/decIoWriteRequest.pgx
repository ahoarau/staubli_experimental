  // limitations under the License.



  // decode message ID 65202 body (data)
  // assuming contains only one item!

  // assume failure to decode
  //rosIoWriteRequestAck.header.nReplyCode=2

  
  
  logMsg("decodeStart")
  
  
  
  
  // count
  l_nRetVal=fromBinary(rosIoWriteRequestMsg.body.nData,4,"-4l",rosIoWriteRequestMsg.ioWriteRequest.nCount)
  if (l_nRetVal!=1)
    x_bFlag=false
    return
  endIf

  logMsg("decode1")
  // type
  
    l_nRetVal=fromBinary(rosIoWriteRequestMsg.body.nData[4],4,"-4l",rosIoWriteRequestMsg.ioWriteRequest.nType)
    if (l_nRetVal!=1)
      x_bFlag=false
      return
    endIf
  
  logMsg("decode2")
  
  // index
  l_nRetVal=fromBinary(rosIoWriteRequestMsg.body.nData[8],4,"-4l",rosIoWriteRequestMsg.ioWriteRequest.nIndex)
  if (l_nRetVal!=1)
    x_bFlag=false
    return
  endIf

  logMsg("decode3")
  
  // value
  l_nRetVal=fromBinary(rosIoWriteRequestMsg.body.nData[12],4,"-4l",rosIoWriteRequestMsg.ioWriteRequest.nValue)
  if (l_nRetVal!=1)
    x_bFlag=false
    return
  endIf

  logMsg("decode success")
  
  // decoding successful
//  rosIoWriteReqAck.header.nReplyCode=1
//  rosIoWriteReqAck.jointTrajPt.nSeq=rosIoWriteRequestMsg.ioWriteRequest.nSeq
  x_bFlag=true
end]]></Code>
  </Program>
</Programs>
